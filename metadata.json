{
  "name": "spacepants-zookeeper",
  "version": "0.1.0",
  "author": "Paul Bailey",
  "summary": "Module to install and configure a zookeeper cluster",
  "license": "Apache-2.0",
  "source": "https://github.com/spacepants/puppet-zookeeper.git",
  "project_page": "https://github.com/spacepants/puppet-zookeeper",
